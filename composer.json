{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ashallendesign/favicon-fetcher": "^3.0",
        "bensampo/laravel-enum": "^6.2",
        "echolabsdev/prism": "^0.22.0",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^11.31",
        "laravel/horizon": "^5.30",
        "laravel/pennant": "^1.14",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.9",
        "lorisleiva/laravel-actions": "^2.8",
        "spatie/laravel-login-link": "^1.1",
        "tightenco/ziggy": "^2.4",
        "willvincent/feeds": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^3.3",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^11.0.1",
        "wulfheart/laravel-actions-ide-helper": "^0.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@ide-helper"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -W -R",
            "@php artisan ide-helper:actions",
            "@pint"
        ],
        "pint": [
            "vendor/bin/pint"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -l 1 app"
        ],
        "lint": [
            "@pint",
            "@phpstan"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "composer install",
            "npm install",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan horizon\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
