{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ashallendesign/favicon-fetcher": "^1.3",
        "bensampo/laravel-enum": "^6.2",
        "brendt/php-sparkline": "^1.0",
        "doctrine/dbal": "^3.5",
        "gioni06/gpt3-tokenizer": "^1.1",
        "guzzlehttp/guzzle": "^7.9",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.13",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^9.8",
        "laravel/telescope": "^4.11",
        "laravel/tinker": "^2.7",
        "lorisleiva/laravel-actions": "^2.4",
        "meilisearch/meilisearch-php": "^0.27.0",
        "openai-php/laravel": "^0.10.0",
        "spatie/laravel-login-link": "^1.1",
        "tightenco/ziggy": "^1.0",
        "willvincent/feeds": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.17",
        "laravel/pint": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan horizon:publish --ansi",
            "@ide-helper"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -W -r",
            "@pint"
        ],
        "pint": [
            "vendor/bin/pint"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -l 1 app"
        ],
        "lint": [
            "@pint",
            "@phpstan"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
